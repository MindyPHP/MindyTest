{
  "name": "mindy/sentry-bundle",
  "type": "library",
  "description": "Symfony integration for Sentry (http://getsentry.com)",
  "keywords": [
    "logging",
    "errors",
    "symfony",
    "sentry"
  ],
  "homepage": "http://getsentry.com",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "David Cramer",
      "email": "dcramer@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "sentry/sentry": ">=1.5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.6.6",
    "symfony/dependency-injection": "~3.0",
    "symfony/http-kernel": "~3.0",
    "symfony/security": "~3.0",
    "symfony/config": "~3.0",
    "symfony/console": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "Mindy\\Bundle\\SentryBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mindy\\Bundle\\SentryBundle\\Test\\": ""
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.7.x-dev"
    }
  }
}
