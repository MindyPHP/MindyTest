{
    "name": "mindy/orm-bundle",
    "type": "symfony-bundle",
    "description": "Mindy OrmBundle",
    "keywords": [
        "orm"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Falaleev Maxim",
            "email": "max@studio107.ru"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "mindy/orm": "~3.0",
        "doctrine/migrations": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "5.3.4"
    },
    "autoload": {
        "psr-4": {
            "Mindy\\Bundle\\OrmBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "3.6.4-dev"
        }
    }
}